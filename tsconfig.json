{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/assets/*": [
        "./assets/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    ".expo",
    "android",
    "ios",
    "dist"
  ]
}
